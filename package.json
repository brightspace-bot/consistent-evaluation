{
  "name": "d2l-consistent-evaluation",
  "description": "A consistent evaluation page for all tools",
  "version": "0.0.48",
  "repository": "https://github.com/BrightspaceHypermediaComponents/consistent-evaluation.git",
  "private": true,
  "scripts": {
    "lint": "npm run lint:eslint && npm run lint:lit",
    "lint:eslint": "eslint . --ext .js,.html",
    "lint:lit": "lit-analyzer consistent-evaluation.js components demo test",
    "start": "es-dev-server --app-index demo/index.html --node-resolve --dedupe --open --watch",
    "test": "npm run lint && npm run test:headless",
    "test:headless": "karma start",
    "test:headless:watch": "karma start --auto-watch=true --single-run=false",
    "test:sauce": "karma start karma.sauce.conf.js",
    "test:diff": "mocha test/**/*.visual-diff.js -t 40000",
    "test:diff:golden": "mocha test/**/*.visual-diff.js -t 40000 --golden",
    "test:diff:golden:commit": "commit-goldens"
  },
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "@brightspace-ui/visual-diff": "^1.1.1",
    "@open-wc/testing": "^2",
    "@open-wc/testing-karma": "^3",
    "babel-eslint": "^10",
    "deepmerge": "^3",
    "es-dev-server": "^1.38.3",
    "eslint": "^6",
    "eslint-config-brightspace": "^0.6.6",
    "eslint-plugin-html": "^6",
    "eslint-plugin-lit": "^1",
    "eslint-plugin-sort-class-members": "^1",
    "frau-ci": "^1",
    "karma-sauce-launcher": "^2",
    "lit-analyzer": "^1",
    "mocha": "^7.0.1",
    "puppeteer": "^1",
    "sinon": "^9.0.2",
    "siren-parser": "^8.2.0"
  },
  "dependencies": {
    "@adobe/lit-mobx": "0.0.x",
    "@brightspace-ui-labs/grade-result": "^1.0.5",
    "@brightspace-ui/core": "^1.36.0",
    "@webcomponents/webcomponentsjs": "^2",
    "d2l-activities": "github:BrightspaceHypermediaComponents/activities#semver:^3",
    "d2l-html-editor": "github:Brightspace/d2l-html-editor#semver:^2",
    "d2l-outcomes-level-of-achievements": "github:Brightspace/outcomes-level-of-achievement-ui#semver:^2",
    "d2l-rubric": "github:Brightspace/d2l-rubric#semver:^3",
    "d2l-tooltip": "github:BrightspaceUI/tooltip#semver:^3",
    "lit-element": "^2",
    "mobx": "^5.15.4"
  }
}
